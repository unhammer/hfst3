FORMAT=TRANSDUCER_FORMAT

# identity
echo "[?]" | hfst-regexp2fst -f $FORMAT > Solution

for i in '[ D -> N^2, Q -> N^5 ]' \
         'N^5/\N @-> ...  "[COLA]"' \
  	 'N -> 0 || _ $"[COLA]"' \
	 'N^2/\N @-> ...  D' \
	 'N -> 0 || _ $D'; do
  echo $i | hfst-regexp2fst -f $FORMAT | hfst-compose -1 Solution > TMP;
  mv TMP Solution;
done

mv Solution BetterColaMachine.hfst.hfst
