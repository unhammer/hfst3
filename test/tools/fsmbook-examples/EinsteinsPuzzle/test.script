hfst-xfst -f FOO.xfst.script

for i in sfst openfst-tropical foma; do
  sed -i "1s/TRANSDUCER_FORMAT/"$i"/;" FOO.hfst.script
  echo "Testing" $i "..."
  sh FOO.hfst.script
  sed -i "1s/"$i"/TRANSDUCER_FORMAT/;" FOO.hfst.script
  hfst-fst2fst -f openfst-tropical FOO.hfst.hfst > TMP
  hfst-compare -q TMP FOO.xfst.hfst && echo "TEST PASSED";
done
